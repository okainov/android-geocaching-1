<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
            android:title="@string/map_category_title"> 
            
        <CheckBoxPreference android:title="Традиционный"
                            android:key="@string/cache_filter_traditional"
                            android:defaultValue="@bool/cache_filter_traditional_default_value"/>
            
        <CheckBoxPreference android:title="Виртуальный"
                            android:key="@string/cache_filter_virtual"
                            android:defaultValue="@bool/cache_filter_virtual_default_value"/>
            
        <CheckBoxPreference android:title="Пошаговый"
                            android:key="@string/cache_filter_stepbystep"
                            android:defaultValue="@bool/cache_filter_stepbystep_default_value"/>
            
        <CheckBoxPreference android:title="Экстрим"
                            android:key="@string/cache_filter_extreme"
                            android:defaultValue="@bool/cache_filter_extreme_default_value"/>
            
        <CheckBoxPreference android:title="Событие"
                            android:defaultValue="@bool/cache_filter_event_default_value"
                            android:key="@string/cache_filter_event"/>
            
        <CheckBoxPreference android:title="Доступный"
                            android:key="@string/cache_filter_valid"
                            android:defaultValue="@bool/cache_filter_valid_default_value"/>
            
        <CheckBoxPreference android:title="Возможно недоступный"
                            android:key="@string/cache_filter_not_confirmed"
                            android:defaultValue="@bool/cache_filter_not_confirmed_default_value"/>
            
        <CheckBoxPreference android:title="Недоступный"
                            android:key="@string/cache_filter_not_valid"
                            android:defaultValue="@bool/cache_filter_not_valid_default_value"/>
    </PreferenceCategory>
    
    
</PreferenceScreen>