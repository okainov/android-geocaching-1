<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
        android:title="Image folders to monitor" android:dialogTitle="Image folders to monitor">
    <PreferenceCategory
            android:title="@string/cache_filter_status_title">

        <CheckBoxPreference android:title="@string/cache_filter_valid_title"
                            android:defaultValue="@bool/cache_filter_valid_default_value"
                            android:key="@string/cache_filter_valid"/>

        <CheckBoxPreference android:title="@string/cache_filter_not_confirmed_title"
                            android:defaultValue="@bool/cache_filter_not_confirmed_default_value"
                            android:key="@string/cache_filter_not_confirmed"/>

        <CheckBoxPreference android:title="@string/cache_filter_not_valid_title"
                            android:defaultValue="@bool/cache_filter_not_valid_default_value"
                            android:key="@string/cache_filter_not_valid"/>
    </PreferenceCategory>


</PreferenceScreen>
